2212target%=2020-K%
2310FOR I%=0 TO nv% DIV 2 - 1
2410IF FNhas_number(target%-av%(I%)) THEN PRO.found(av%(K%),av%(I%))
2510NEXT
2511NEXT
2610PRINT TIME-start
2710END
